<body>
  <div class="page-header text-center">
    <a href="/logout" class="btn btn-default btn-sm">Logout</a>
  </div>

    <div class="container">
      <h1>Ready to be suprised? <br>Enter Date available & Budget</h1>
        <!-- <section id="image"> -->


    <div class="row">
      <!-- Search -->
            <% if (user.local.email) { %>
                <form class="searchinputs" action="/doors" method="post">
                  <!-- <input type="text" name="city" value="" placeholder="city" required="required"> -->
                    <!-- <input type="text" name="departure" class="search_input search_input_2" placeholder="Departure" required="required"> -->
                    <!-- <input type="text" name="arrival" class="search_input search_input_3" placeholder="Arrival" required="required"> -->

                    <span>Departure Date </span>
                    <input id="departureDate" type="date" name="date" class="search_input search_input_1" placeholder="YYYY-MM-DD" required="required">
                    <span>Budget</span>
                    <!-- <input id="budget" type="text" name="budget" class="search_input search_input_2" placeholder="Budget" required="required"> -->
                    <span class="slideContainer">
                      <input id="budget" type="range" list="tickmarks" min="150" max="2000" name="budget" class="search_input search_input_2" placeholder="Budget" required="required">
                      <datalist id="tickmarks">
                        <option value="150">
                        <option value="200">
                        <option value="500">
                        <option value="1000">
                        <option value="2000">
                      </datalist>
                    </span>


                    <button class="home_search_button" type="submit">search</button>
                  </form>
      <!-- LOCAL INFORMATION -->
        <% } %>
      </div>
    </div>


     <!-- Section: Icon Boxes -->
       <section class="section section-icons grey lighten-4 center">
         <div class="container">
           <div class="row">
             <div class="col s12 m4">
               <div class="card-panel">
                 <i class="material-icons large teal-text">room</i>
                 <h4>Pick Region</h4>
                 <p>Lorem, ipsum dolor sit amet consectetur adipisicing elit. Accusamus, tempore?</p>
               </div>
             </div>
             <div class="col s12 m4">
               <div class="card-panel">
                 <i class="material-icons large teal-text">store</i>
                 <h4>Surprise country</h4>
                 <p>Lorem, ipsum dolor sit amet consectetur adipisicing elit. Accusamus, tempore?</p>
               </div>
             </div>
             <div class="col s12 m4">
               <div class="card-panel">
                 <i class="material-icons large teal-text">airplanemode_active</i>
                 <h4>Fly Me Away</h4>
                 <p>Lorem, ipsum dolor sit amet consectetur adipisicing elit. Accusamus, tempore?</p>
               </div>
             </div>
           </div>
         </div>
       </section>

       <!-- boxes-images -->

       <section id="gallery" class="section section-gallery scrollspy">
          <div class="container">
            <h4 class="center">
              <span class="teal-text">Photo</span> Gallery
            </h4>
            <div class="row">
              <div class="col s12 m3">
                <img src="https://source.unsplash.com/1600x900/?mountain" alt="" class="materialboxed responsive-img">
              </div>
              <div class="col s12 m3">
